local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
    Name = "Rayfield Example Window",
    Icon = 0, -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
    LoadingTitle = "Rayfield Interface Suite",
    LoadingSubtitle = "by Sirius",
    Theme = "Default", -- Check https://docs.sirius.menu/rayfield/configuration/themes
 
    DisableRayfieldPrompts = false,
    DisableBuildWarnings = false, -- Prevents Rayfield from warning when the script has a version mismatch with the interface
 
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nil, -- Create a custom folder for your hub/game
       FileName = "roKu Hub"
    },
 
    Discord = {
       Enabled = false, -- Prompt the user to join your Discord server if their executor supports it
       Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ ABCD would be ABCD
       RememberJoins = true -- Set this to false to make them join the discord every time they load it up
    },
 
    KeySystem = false, -- Set this to true to use our key system
    KeySettings = {
       Title = "Untitled",
       Subtitle = "Key System",
       Note = "No method of obtaining the key is provided", -- Use this to tell the user how to get a key
       FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
       SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
       GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
       Key = {"123"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
    }
 })
 local Tab = Window:CreateTab("Main", 4483362458)
 local Section = Tab:CreateSection("Aimbot")

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local mouse = player:GetMouse()
local camera = workspace.CurrentCamera

local aimEnabled = false
local MAX_DISTANCE = 65
local AIM_SMOOTHNESS = 0.3

local BLOCKED_NAMES = { -- âŒ à¸Šà¸·à¹ˆà¸­à¸—à¸µà¹ˆà¹„à¸¡à¹ˆà¸¥à¹‡à¸­à¸
    ["1tygho_134"] = true
}

-- à¸«à¸²à¹€à¸›à¹‰à¸²à¸«à¸¡à¸²à¸¢à¹ƒà¸à¸¥à¹‰à¸ªà¸¸à¸”
local function getClosestTarget()
    local closestTarget = nil
    local shortestDistance = math.huge

    for _, target in ipairs(Players:GetPlayers()) do
        if target ~= player
        and not BLOCKED_NAMES[target.Name]
        and target.Character
        and target.Character:FindFirstChild("Head") then

            local head = target.Character.Head
            if head and head:IsA("BasePart") and target.Character:FindFirstChild("Humanoid") and target.Character.Humanoid.Health > 0 then
                local headPos, onScreen = camera:WorldToViewportPoint(head.Position)

                if onScreen then
                    local mousePos = Vector2.new(mouse.X, mouse.Y)
                    local distance = (mousePos - Vector2.new(headPos.X, headPos.Y)).Magnitude
                    local distance3D = (camera.CFrame.Position - head.Position).Magnitude

                    if distance < shortestDistance and distance3D <= MAX_DISTANCE then
                        shortestDistance = distance
                        closestTarget = target
                    end
                end
            end
        end
    end

    return closestTarget
end

-- à¸­à¸±à¸žà¹€à¸”à¸—à¸à¸¥à¹‰à¸­à¸‡à¸•à¸²à¸¡à¹€à¸›à¹‰à¸²
RunService.RenderStepped:Connect(function()
    if aimEnabled then
        local target = getClosestTarget()
        if target and target.Character and target.Character:FindFirstChild("Head") then
            local headPos = target.Character.Head.Position
            local newCFrame = CFrame.new(camera.CFrame.Position, headPos)
            camera.CFrame = camera.CFrame:Lerp(newCFrame, AIM_SMOOTHNESS)
        end
    end
end)

-- ðŸŒŸ Rayfield UI Toggle
local Toggle = Tab:CreateToggle({
   Name = "Aimbot à¹€à¸¥à¹‡à¸‡à¸«à¸±à¸§ (à¸¡à¸µà¸„à¸µà¸¢à¹Œà¸¥à¸±à¸”)",
   CurrentValue = false,
   Flag = "StableAimbotWithKeybind",
   Callback = function(Value)
      aimEnabled = Value
   end,
})

-- ðŸŽ¯ à¸£à¸°à¸šà¸šà¸„à¸µà¸¢à¹Œà¸¥à¸±à¸” (Left Control à¹€à¸›à¸´à¸”/à¸›à¸´à¸”)
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.LeftControl then
        aimEnabled = not aimEnabled -- à¹€à¸›à¸´à¸”/à¸›à¸´à¸”
        Toggle:Set(aimEnabled) -- à¸­à¸±à¸žà¹€à¸”à¸— UI Toggle à¸”à¹‰à¸§à¸¢
    end
end)


local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local localPlayer = Players.LocalPlayer
local espEnabled = false

-- ðŸ“¦ à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¸ªà¸£à¹‰à¸²à¸‡ ESP (Highlight)
local function createESP(character)
    if not character:FindFirstChild("ESP_Highlight") then
        local highlight = Instance.new("Highlight")
        highlight.Name = "ESP_Highlight"
        highlight.FillColor = Color3.fromRGB(255, 0, 0) -- à¸ªà¸µà¸•à¸±à¸§ ESP
        highlight.OutlineColor = Color3.fromRGB(255, 255, 255) -- à¸ªà¸µà¸‚à¸­à¸š
        highlight.FillTransparency = 0.5
        highlight.OutlineTransparency = 0
        highlight.Adornee = character
        highlight.Parent = character
    end
end

-- ðŸ—‘ï¸ à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¸¥à¸š ESP
local function removeESP(character)
    local highlight = character:FindFirstChild("ESP_Highlight")
    if highlight then
        highlight:Destroy()
    end
end

-- ðŸŽ¯ à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¸­à¸±à¸›à¹€à¸”à¸• ESP à¹ƒà¸«à¹‰à¸à¸±à¸šà¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™à¸„à¸™à¹€à¸”à¸µà¸¢à¸§
local function updateESP(player)
    if player ~= localPlayer then
        if player.Character then
            if espEnabled then
                createESP(player.Character)
            else
                removeESP(player.Character)
            end
        end
    end
end

-- âš¡ à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¹€à¸Šà¸·à¹ˆà¸­à¸¡à¸•à¹ˆà¸­à¹€à¸¡à¸·à¹ˆà¸­à¸•à¸±à¸§à¸¥à¸°à¸„à¸£à¸‚à¸­à¸‡à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™à¹€à¸à¸´à¸”
local function setupPlayer(player)
    -- à¸–à¹‰à¸²à¸¡à¸µà¸•à¸±à¸§à¸¥à¸°à¸„à¸£à¹à¸¥à¹‰à¸§
    if player.Character then
        wait(0.5) -- à¸£à¸­à¹‚à¸«à¸¥à¸”à¸•à¸±à¸§à¹€à¸ªà¸£à¹‡à¸ˆà¸à¹ˆà¸­à¸™
        updateESP(player)
    end

    -- à¸–à¹‰à¸²à¸¡à¸µà¸à¸²à¸£à¹€à¸à¸´à¸”à¸•à¸±à¸§à¹ƒà¸«à¸¡à¹ˆ
    player.CharacterAdded:Connect(function(character)
        character:WaitForChild("HumanoidRootPart") -- à¸£à¸­à¹‚à¸«à¸¥à¸”
        wait(0.5)
        updateESP(player)
    end)
end

-- ðŸ› ï¸ à¸•à¸´à¸”à¸•à¸±à¹‰à¸‡ ESP à¹ƒà¸«à¹‰à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™à¸—à¸µà¹ˆà¸¡à¸µà¸­à¸¢à¸¹à¹ˆà¹à¸¥à¹‰à¸§
for _, player in ipairs(Players:GetPlayers()) do
    setupPlayer(player)
end

-- ðŸ› ï¸ à¹€à¸¡à¸·à¹ˆà¹ˆà¸­à¸¡à¸µà¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™à¹ƒà¸«à¸¡à¹ˆà¹€à¸‚à¹‰à¸²à¸¡à¸²
Players.PlayerAdded:Connect(function(player)
    setupPlayer(player)
end)

-- ðŸŸ¢ à¹ƒà¸ªà¹ˆ Toggle UI (à¹€à¸«à¸¡à¸·à¸­à¸™ Aimbot Toggle)
local Toggle = Tab:CreateToggle({
   Name = "ESP (à¸¡à¸­à¸‡à¹€à¸«à¹‡à¸™à¸¨à¸±à¸•à¸£à¸¹)",
   CurrentValue = false,
   Flag = "ESP_Toggle",
   Callback = function(Value)
      espEnabled = Value
      -- à¸­à¸±à¸›à¹€à¸”à¸• ESP à¸—à¸¸à¸à¸„à¸™à¹€à¸¡à¸·à¹ˆà¸­à¹€à¸›à¸´à¸”/à¸›à¸´à¸”
      for _, player in ipairs(Players:GetPlayers()) do
          updateESP(player)
      end
   end,
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local WalkSpeed_Normal = 16 -- à¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§à¹€à¸”à¸´à¸™à¸›à¸à¸•à¸´
local WalkSpeed_Fast = 50 -- à¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§à¸•à¸­à¸™à¹€à¸›à¸´à¸”à¸ªà¸›à¸µà¸”

local speedEnabled = false -- à¸ªà¸–à¸²à¸™à¸°à¹€à¸›à¸´à¸”/à¸›à¸´à¸”à¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¸›à¸£à¸±à¸šà¸ªà¸›à¸µà¸”
local function updateSpeed()
    if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        if speedEnabled then
            humanoid.WalkSpeed = WalkSpeed_Fast
        else
            humanoid.WalkSpeed = WalkSpeed_Normal
        end
    end
end

-- à¸•à¸±à¸§à¸¥à¸°à¸„à¸£à¹€à¸à¸´à¸”à¹ƒà¸«à¸¡à¹ˆ (à¸à¸±à¸™à¸•à¸²à¸¢à¹à¸¥à¹‰à¸§à¸ªà¸›à¸µà¸”à¸«à¸²à¸¢)
player.CharacterAdded:Connect(function()
    wait(1)
    updateSpeed()
end)

-- à¹€à¸‹à¹‡à¸•à¸ªà¸›à¸µà¸”à¸—à¸±à¸™à¸—à¸µà¸•à¸­à¸™à¹€à¸£à¸´à¹ˆà¸¡
if player.Character then
    updateSpeed()
end

-- âœ… Rayfield UI Toggle
local Toggle = Tab:CreateToggle({
    Name = "à¹€à¸›à¸´à¸”/à¸›à¸´à¸” à¸§à¸´à¹ˆà¸‡à¹„à¸§",
    CurrentValue = false,
    Flag = "FastWalkToggle",
    Callback = function(Value)
        speedEnabled = Value
        updateSpeed()
    end,
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer
local Rayfield = loadstring(game:HttpGet("https://raw.githubusercontent.com/averydev/Rayfield/main/source.lua"))()

local ESPFolder = Instance.new("Folder")
ESPFolder.Name = "ESPFolder"
ESPFolder.Parent = game.CoreGui -- à¸§à¸²à¸‡à¸—à¸µà¹ˆ CoreGui

local speedEnabled = false -- à¹ƒà¸Šà¹‰à¹€à¸›à¸´à¸”/à¸›à¸´à¸” ESP

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¸ªà¸£à¹‰à¸²à¸‡ ESP
local function createESP(targetPlayer)
    if targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "ESP"
        billboard.Adornee = targetPlayer.Character.Head
        billboard.Size = UDim2.new(4, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true
        billboard.Parent = ESPFolder

        -- à¸Šà¸·à¹ˆà¸­à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(1, 0, 1, 0)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = targetPlayer.Name -- à¸Šà¸·à¹ˆà¸­à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™
        nameLabel.TextColor3 = Color3.fromRGB(255, 0, 0) -- à¸ªà¸µà¹à¸”à¸‡
        nameLabel.TextStrokeTransparency = 0
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.TextScaled = true
        nameLabel.Parent = billboard
    end
end

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¸¥à¸š ESP
local function removeESP(targetPlayer)
    for _, esp in pairs(ESPFolder:GetChildren()) do
        if esp:IsA("BillboardGui") and esp.Adornee and esp.Adornee.Parent and Players:GetPlayerFromCharacter(esp.Adornee.Parent) == targetPlayer then
            esp:Destroy()
        end
    end
end

-- à¹ƒà¸ªà¹ˆ ESP à¹ƒà¸«à¹‰à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™à¸—à¸µà¹ˆà¸¡à¸µà¸­à¸¢à¸¹à¹ˆà¹à¸¥à¹‰à¸§
for _, target in ipairs(Players:GetPlayers()) do
    if target ~= player then
        createESP(target)
    end
end

-- à¹€à¸¡à¸·à¹ˆà¸­à¸¡à¸µà¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™à¹ƒà¸«à¸¡à¹ˆà¹€à¸‚à¹‰à¸²à¸¡à¸²
Players.PlayerAdded:Connect(function(newPlayer)
    newPlayer.CharacterAdded:Connect(function()
        wait(1) -- à¸£à¸­à¹‚à¸«à¸¥à¸”à¸•à¸±à¸§
        if speedEnabled then
            createESP(newPlayer)
        end
    end)
end)

-- à¹€à¸¡à¸·à¹ˆà¸­à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™à¸­à¸­à¸
Players.PlayerRemoving:Connect(function(leavingPlayer)
    removeESP(leavingPlayer)
end)

-- âœ… Rayfield UI Toggle à¸ªà¸³à¸«à¸£à¸±à¸šà¹€à¸›à¸´à¸”/à¸›à¸´à¸” ESP
local Tab = Rayfield:CreateTab({
    Name = "ESP Controller",
    Icon = "rbxassetid://4483345998", -- Icon à¸ˆà¸²à¸ Roblox
    PremiumOnly = false
})

local Toggle = Tab:CreateToggle({
    Name = "à¹€à¸›à¸´à¸”/à¸›à¸´à¸” ESP",
    CurrentValue = false,
    Flag = "ESP_Toggle", -- à¹ƒà¸«à¹‰ Flag à¹€à¸œà¸·à¹ˆà¸­à¹ƒà¸Šà¹‰à¹ƒà¸™à¸à¸²à¸£à¹€à¸‹à¸Ÿà¸„à¹ˆà¸²à¸œà¹ˆà¸²à¸™ Rayfield
    Callback = function(Value)
        speedEnabled = Value

        -- à¸–à¹‰à¸²à¹€à¸›à¸´à¸” ESP, à¹ƒà¸«à¹‰à¸—à¸³à¸à¸²à¸£à¸ªà¸£à¹‰à¸²à¸‡ ESP à¹ƒà¸«à¸¡à¹ˆ
        if speedEnabled then
            -- à¹ƒà¸ªà¹ˆ ESP à¹ƒà¸«à¹‰à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™à¸—à¸µà¹ˆà¸¡à¸µà¸­à¸¢à¸¹à¹ˆà¹à¸¥à¹‰à¸§
            for _, target in ipairs(Players:GetPlayers()) do
                if target ~= player then
                    createESP(target)
                end
            end
        else
            -- à¸–à¹‰à¸²à¸›à¸´à¸” ESP, à¸¥à¸šà¸—à¸¸à¸à¸•à¸±à¸§ ESP
            for _, esp in pairs(ESPFolder:GetChildren()) do
                esp:Destroy()
            end
        end
    end
})

